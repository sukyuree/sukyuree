        app:layout_constraintTop_toTopOf="parent" />

    <ListView
        android:id="@+id/Tues"
        android:layout_width="83dp"
        android:layout_height="135dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="180dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/sdsamliphopangchebasic"
        app:layout_constraintStart_toEndOf="@+id/mon"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tuesday"
        android:layout_width="83dp"
        android:layout_height="42dp"
        android:background="#A2E159"
        android:fontFamily="@font/sdsamliphopangchebasic"
        android:text="       T"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintBottom_toTopOf="@+id/Tues"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.365"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.963" />

    <TextView
        android:id="@+id/WednesDay"
        android:layout_width="83dp"
        android:layout_height="42dp"
        android:background="#A2E159"
        android:fontFamily="@font/sdsamliphopangchebasic"
        android:text="       W"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintBottom_toTopOf="@+id/W"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.667"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.956" />

    <ListView
        android:id="@+id/W"
        android:layout_width="83dp"
        android:layout_height="135dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="180dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/sdsamliphopangchebasic"
        app:layout_constraintStart_toEndOf="@+id/Tues"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>